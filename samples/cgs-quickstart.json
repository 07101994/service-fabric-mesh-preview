{
    "instanceCount": 2,
    "containerGroup": {
        "containers": [
        {
            "name": "sbz-quickstart-app",
            "properties": {
            "image": "seabreeze/sbz-helloworld:1.0-alpine",
            "resources": {
                "requests": {
                "cpu": 1,
                "memoryInGb": 1.5
                }
            },
            "ports": [
                {
                "port": 80
                }
            ]
            }
        },
        {
            "name": "sbz-quickstart-sidecar",
            "properties": {
            "image": "seabreeze/sbz-helloworld-sidecar:1.0-alpine",
            "resources": {
                "requests": {
                "cpu": 1,
                "memoryInGb": 1.5
                }
            },
            "environmentVariables":[
                {
                "name": "foo",
                "value": "bar"
                }
            ]
            }
        }
        ],
        "osType": "Linux",
        "ipAddress": {
        "type": "Public",
        "ports": [
            {
            "protocol": "tcp",
            "port": "80"
            }
        ]
        }
    }
}